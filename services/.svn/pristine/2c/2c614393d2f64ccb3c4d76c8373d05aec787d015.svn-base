  <select resultType="java.math.BigDecimal" parameterType="java.util.Map" id="getTotalMoney" >
    select ifnull(sum(order_amt),0) totalOrderAmt from t_order_pay where update_time like CONCAT(#{date},'%') 
    and pay_status='02'
    <if test="cashierNo != null and cashierNo != '' " >
    and cashier_no=#{cashierNo}
    </if>
    <if test="merchantNo != null and merchantNo != '' " >
    and merchant_no=#{merchantNo}
    </if>
  </select>
  
  <select resultType="map" parameterType="java.util.Map" id="getDealMoney" >
    select sum(ifnull(order_amt,0)) total,sum(ifnull(order_amt,0)-IFNULL(merchant_cost,0)) as balance 
    from t_order_pay where update_time like CONCAT(#{date},'%') and pay_status='02' 
    <if test="cashierNo != null and cashierNo != '' " >
    and cashier_no=#{cashierNo}
    </if>
    <if test="merchantNo != null and merchantNo != '' " >
    and merchant_no=#{merchantNo}
