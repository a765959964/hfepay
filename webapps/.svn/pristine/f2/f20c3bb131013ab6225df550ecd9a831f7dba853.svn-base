	request.setAttribute("path", path);
%>
<table class="table table-striped table-bordered" id="sample_6">

	<tr>
		<td class="col-md-2 td0">渠道名称</td><td class="col-md-4 parent_id_parse" title="code">${item.channelName}</td>
		<td class="col-md-2 td0">渠道编号</td><td class="col-md-4 parent_id_parse" title="number">${item.channelNo}</td>
	</tr>
	<tr>
		<td class="col-md-2 td0">二级域名编号</td><td colspan="3">${item.channelCode}</td>
	</tr>
	<tr>
		<td class="col-md-2 td0">昵称</td><td class="col-md-4 parent_id_parse" title="nickName">${item.nickName}</td>
		<td class="col-md-2 td0">编码抬头</td><td class="col-md-4 parent_id_parse" title="preCode">${item.channelPreCode}</td>
	</tr>
